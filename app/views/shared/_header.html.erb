<%= link_to 'Home', root_path %>
<% if session['token'] %>
  <%= link_to 'Profile', profile_path(id: "#{session['user_id']}") %>
  <%= link_to 'Logout', user_logout_path, method: :delete, data: { confirm: 'Are you sure?' } %>
<% else %>
  <%= link_to 'Sign up', user_register_path %>
  <%= link_to 'Login', user_login_path %>
<% end %>